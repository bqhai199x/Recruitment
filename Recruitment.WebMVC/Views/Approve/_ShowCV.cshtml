<!-- Modal -->

<div class="modal-header">
    <h5 class="modal-title" id="staticBackdropLabel">Duyệt CV ứng viên @ViewBag.Name</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <embed src="~/wwwroot/CV/@ViewBag.CV" frameborder="0" width="100%" height="600px>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-success">Duyệt</button>
    <button type="button" class="btn btn-danger" data-dismiss="modal">Loại</button>
</div>
